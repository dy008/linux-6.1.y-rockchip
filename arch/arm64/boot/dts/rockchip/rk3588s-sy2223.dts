&i2c3 {
	pinctrl-names = "default";
	pinctrl-0 = <&i2c3m0_xfer>;
	status = "okay";

	es8323: es8323@11 {
		status = "okay";
		#sound-dai-cells = <0>;
		compatible = "everest,es8323";
		reg = <0x11>;
		adc-pga-gain = <0x06>;
		adc-volume = <0xbf>;
		dac-volume = <0xbf>;
		aec-mode = "adc left, adc right";
		clocks = <&mclkout_i2s0>;
		clock-names = "mclk";
		spk-ctl-gpio = <&gpio4 RK_PA5 GPIO_ACTIVE_HIGH>;
		assigned-clocks = <&mclkout_i2s0>;
		assigned-clock-rates = <12288000>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2s0_mclk>;
	};
};

&i2c5 {
	pinctrl-names = "default";
	pinctrl-0 = <&i2c5m2_xfer>;
