/ {
	model = "ECOVACS RK3588 Hl Development Board";
	compatible = "rockchip,sy2223-rk3588s", "rockchip,rk3588";

	/delete-node/ chosen;

	vcc12v_dcin: vcc12v-dcin {
		compatible = "regulator-fixed";
		regulator-name = "vcc12v_dcin";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <12000000>;
		regulator-max-microvolt = <12000000>;
	};

	vcc5v0_sys: vcc5v0-sys {
		compatible = "regulator-fixed";
		regulator-name = "vcc5v0_sys";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		vin-supply = <&vcc12v_dcin>;
	};

	vcc_1v1_nldo_s3: vcc-1v1-nldo-s3 {
		compatible = "regulator-fixed";
		regulator-name = "vcc_1v1_nldo_s3";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <1100000>;
		regulator-max-microvolt = <1100000>;
		vin-supply = <&vcc5v0_sys>;
	};

	vcc-dphy0 {
		compatible = "regulator-fixed";
		enable-active-high;
		//gpio = <0x159 0x09 0x00>;
		//gpio = <&gpio1 RK_PB1 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&dphy0_pwr>;
		regulator-name = "vcc_dphy0";
	};

	vcc_usb2_power {
		//左后单USB3.0座VBUS控制
		compatible = "regulator-fixed";
		regulator-name = "vcc_usb2";
		enable-active-high;
		regulator-boot-on;
		//enable-gpio = <0x159 0x04 0x00>;
		enable-gpio = <&gpio1 RK_PA4 GPIO_ACTIVE_HIGH>;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		pinctrl-names = "default";
		pinctrl-0 = <&usb_hub2_en>;
	};

	usb_hub_reset1 {
		// U9608(左侧CYUSB3304 USB HUB复位控制 高电平复位)
		compatible = "regulator-fixed";
		regulator-name = "usb_hub_reset";
		enable-active-high;
		regulator-boot-on;
		//enable-gpio = <0x159 0x08 0x01>;
		enable-gpios = <&gpio1 RK_PB0 GPIO_ACTIVE_HIGH>;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		pinctrl-names = "default";
		pinctrl-0 = <&usb_hub_reset_1>;
		startup-delay-us = <1000000>;
	};

	usb_camera_power1 {
		compatible = "regulator-fixed";
		regulator-name = "usb_camera_power1";
		enable-active-high;
		regulator-boot-on;
		//enable-gpio = <0x159 0x18 0x00>;
		enable-gpio = <&gpio1 RK_PD0 GPIO_ACTIVE_HIGH>;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		pinctrl-names = "default";
		pinctrl-0 = <&usb_camera_power_1>;
	};

	usb_camera_power_2 {
		compatible = "regulator-fixed";
		regulator-name = "usb_camera_power_2";
		enable-active-high;
		regulator-boot-on;
		//enable-gpio = <0x159 0x19 0x00>;
		enable-gpio = <&gpio1 RK_PD1 GPIO_ACTIVE_HIGH>;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		pinctrl-names = "default";
		pinctrl-0 = <&usb_camera_power_2>;
	};

	usb_camera_power_3 {
		compatible = "regulator-fixed";
		regulator-name = "usb_camera_power_3";
